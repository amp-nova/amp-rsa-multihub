{
  "name": "amp-rsa-multihub",
  "scripts": {
    "dev": "nodemon src/index.ts --watch src --ext ts,json,js",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "start": "node dist/index.js",
    "updateSecretBranch": "rm -f /tmp/settings.yaml && mv config/settings.yaml /tmp && git co secret-branch && git merge master -m \"merge\" && git co master && mv /tmp/settings.yaml ./config",
    "deploy": "npm run updateSecretBranch && npm run build && git push heroku secret-branch:master"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.117.0",
    "@aws-cdk/aws-ecs-patterns": "^1.117.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.117.0",
    "@aws-cdk/aws-iam": "^1.117.0",
    "@aws-cdk/aws-route53": "^1.117.0",
    "@aws-cdk/core": "^1.117.0",
    "@aws-sdk/client-dynamodb": "^3.13.1",
    "@aws-sdk/client-s3": "^3.17.0",
    "@aws-sdk/client-secrets-manager": "^3.13.1",
    "amp-rsa-gql": "github:amp-nova/amp-rsa-gql",
    "apollo-server": "^2.21.1",
    "apollo-server-express": "^2.21.2",
    "aws-sdk": "^2.893.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "class-validator": "^0.13.1",
    "currency.js": "^2.0.4",
    "express": "^4.17.1",
    "firebase-admin": "^9.6.0",
    "fs-extra": "^10.0.0",
    "graphql": "^15.5.1",
    "js-yaml": "^4.0.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "slugify": "^1.5.3",
    "source-map-support": "^0.5.19",
    "string-strip-html": "^8.3.0",
    "ts-node": "^9.1.1",
    "type-graphql": "^1.1.1",
    "typescript": "^4.2.3",
    "urijs": "^1.19.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "version": "0.0.7"
}
