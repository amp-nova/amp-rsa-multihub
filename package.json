{
  "name": "@amp-nova/pbx",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "browser": {
    "fs": false
  },
  "browserX": "dist/client/index.js",
  "scripts": {
    "devx": "nodemon src/server/index.ts --watch src --watch package.json --ext ts,js --ignore logs --app_mode dev -r tsconfig-paths/register",
    "dev": "nodemon",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "startPm2": "./node_modules/.bin/pm2 start src/index.ts --name multihub -- --app_mode production",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register src/server/index.ts --app_mode production",
    "test": "ts-node src/test/test.ts",
    "prepare": "npm run build"
  },
  "nodemonConfig": {
    "ignore": [
      "logs",
      "node_modules"
    ],
    "watch": [
      "src",
      "package.json",
      "tsconfig.json"
    ],
    "execA": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
    "exec": "node -r ts-node/register/transpile-only -r tsconfig-paths/register src/server/index.ts",
    "ext": "ts, js"
  },
  "dependencies": {
    "@amp-nova/amp-rsa-types": "^1.0.3",
    "@apollo/client": "^3.4.16",
    "@aws-sdk/client-secrets-manager": "^3.13.1",
    "@aws-sdk/client-translate": "^3.28.0",
    "atob": "^2.1.2",
    "axios": "^0.23.0",
    "class-validator": "^0.13.1",
    "cross-fetch": "^3.1.4",
    "currency.js": "^2.0.4",
    "date-fns": "^2.23.0",
    "dc-management-sdk-js": "^1.13.0",
    "graphql": "^15.5.1",
    "jsonpath": "^1.1.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.25",
    "nconf": "^0.11.3",
    "node-fetch": "^3.0.0",
    "npm": "^8.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "slugify": "^1.5.3",
    "ts-node": "^10.2.1",
    "type-graphql": "^1.1.1",
    "typescript": "^4.2.3",
    "urijs": "^1.19.7",
    "whatwg-url": "^11.0.0",
    "winston": "^3.3.3"
  },
  "version": "0.0.85",
  "devDependencies": {
    "@types/node": "^16.7.2",
    "json-stringify-safe": "^5.0.1",
    "tsconfig-paths": "^3.11.0"
  }
}
